{
  "name": "deep-work-tracker",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Deep Work Tracker: Optimize your productivity with focused work sessions.",
  "keywords": [
    "productivity",
    "deep work",
    "time tracking",
    "focus",
    "nextjs",
    "react",
    "web application"
  ],
  "homepage": "https://deep-work.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/yungKillaGit/deep-work"
  },
  "license": "MIT",
  "volta": {
    "node": "24.11.1"
  },
  "scripts": {
    "dev": "next dev --webpack",
    "build": "next build",
    "start": "next start",
    "preview": "npm run clear-cache && npm run build && npm run start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "clean-install": "rimraf .next node_modules package-lock.json && npm i",
    "clear-cache": "rimraf .next",
    "test:unit": "vitest run --project unit",
    "test": "vitest run",
    "prepare": "husky",
    "screenshot:og-image": "capture-website http://localhost:3000 --output=public/og-image.png --width=1200 --height=630 --overwrite --delay=5",
    "screenshot:app-image": "capture-website http://localhost:3000 --output=public/app-screenshot.png --width=1920 --height=1080 --overwrite --delay=5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "npx vitest run || echo \"No test files found, skipping tests\""
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@mantine/core": "^8.3.10",
    "@mantine/hooks": "^8.3.10",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.89.0",
    "@vercel/speed-insights": "^1.3.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "next": "^16.1.0",
    "pg": "^8.16.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.69.0",
    "react-timer-hook": "^4.0.5",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "^1.0.0",
    "capture-website-cli": "^5.2.0",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unicorn": "^62.0.0",
    "happy-dom": "^20.0.11",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "npm-check-updates": "^19.2.0",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.7.4",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prisma": "^7.2.0",
    "rimraf": "^6.1.2",
    "typescript": "^5",
    "vite": "^7.3.0",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16"
  }
}
